{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Maven: Compile",
			"type": "shell",
			"command": "mvn",
			"args": ["compile"],
			"group": "build",
			"options": {
				"cwd": "${workspaceFolder}/TOOLS/DigitalAssetProtectorWatermarkEngine"
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			},
			"problemMatcher": []
		},
		{
			"label": "Maven: Test",
			"type": "shell",
			"command": "mvn",
			"args": ["test"],
			"group": "test",
			"options": {
				"cwd": "${workspaceFolder}/TOOLS/DigitalAssetProtectorWatermarkEngine"
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			},
			"problemMatcher": []
		},
		{
			"label": "Maven: Clean Compile",
			"type": "shell",
			"command": "mvn",
			"args": ["clean", "compile"],
			"group": "build",
			"options": {
				"cwd": "${workspaceFolder}/TOOLS/DigitalAssetProtectorWatermarkEngine"
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			},
			"problemMatcher": []
		},
		{
			"label": "Maven: Package",
			"type": "shell",
			"command": "mvn",
			"args": ["package"],
			"group": "build",
			"options": {
				"cwd": "${workspaceFolder}/TOOLS/DigitalAssetProtectorWatermarkEngine"
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			},
			"problemMatcher": []
		}
	]
}
